{
  "name": "torrz",
  "productName": "Torrz Torrents",
  "version": "0.1.0",
  "description": "A Modern Torrent Client",
  "author": "Arilson B. <arilsonbrj@gmail.com>",
  "homepage": "./",
  "build": {
    "appId": "ml.tryups.torrz"
  },
  "private": true,
  "main": "./public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "nedb": "^1.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1",
    "typescript": "~3.7.2",
    "webtorrent": "^0.108.6"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.2",
    "@types/webtorrent": "^0.107.2",
    "concurrently": "^5.3.0",
    "electron": "^9.2.0",
    "electron-builder": "^22.8.0",
    "wait-on": "^5.2.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "yarn run react-build && yarn run electron-builder",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "release": "yarn run react-build && yarn run electron-builder --publish=always",
    "rebuild": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}